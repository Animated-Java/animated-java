								"type": "linear",
								"easing": "linear"
							}
						},
						"3.0": {
							"value": ["0.5", "0.5", "0.5"],
							"interpolation": {
								"type": "linear",
								"easing": "ease_out_elastic",
								"easing_arguments": [1.2]
							}
						}
					}
				}
			}
		}
	}
