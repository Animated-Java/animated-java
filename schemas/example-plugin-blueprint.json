{
	"$schema": "./plugin-blueprint.schema.json",
	"format_version": 1,
	"settings": {
		"id": "animated_java:blueprint"
	},
	"textures": {
		"custom_texture": {
			"type": "custom",
			"base64_string": "iVBORw0KGgoAAAANSUhEUgAAAAUA",
			"animation": {
				"frametime": 3,
				"interpolate": true,
				"frames": [0, 2, { "index": 1, "time": 10 }, 3]
