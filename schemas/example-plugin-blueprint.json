				"texture": {
					"0.0": { "default_palette": "default" },
					"1.5": { "default_palette": "alternative" }
				},
				"event": {
					"1.5": { "events": ["event_1", "event_2"] },
					"3.0": { "events": ["event_1"] }
				}
			},
			"node_keyframes": {
				"bone_node": {
