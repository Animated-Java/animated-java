{
	"type": "commonjs",
	"name": "animated_java",
	"title": "Animated Java",
	"icon": "icon.svg",
	"description": "Effortlessly craft complex animations in Minecraft: Java Edition",
	"version": "2.0.0",
	"min_blockbench_version": "5.0.0",
	"variant": "desktop",
