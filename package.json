	"repository": {
		"type": "github",
		"url": "https://github.com/Animated-Java/animated-java"
	},
	"main": "./dist/index.js",
	"types": "./dist/src/index.d.ts",
	"private": true,
	"scripts": {
		"build:scripts": "esbuild --bundle --platform=node --outfile=dist/build.cjs --packages=external ./.scripts/esbuild.ts",
		"dev": "yarn build:scripts && node ./dist/build.cjs --mode=dev",
		"prod": "yarn build:scripts && node ./dist/build.cjs",
		"lint": "eslint . --max-warnings=0",
		"format": "prettier --write .",
		"test": "yarn build:scripts && vitest run",
		"coverage": "yarn build:scripts && vitest run --coverage"
	},
